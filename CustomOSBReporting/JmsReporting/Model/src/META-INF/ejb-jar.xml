<?xml version = '1.0' encoding = 'windows-1252'?>
<ejb-jar xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_0.xsd"
         version="3.0" xmlns="http://java.sun.com/xml/ns/javaee">
  <display-name>QS Message Reporting MDB</display-name>
  <enterprise-beans>
      <message-driven>
          <description>Writer for QS message tracking reporting data</description>
          <ejb-name>OSBReportingMDBBean</ejb-name>
          <security-identity>
                <description>ALSB Security Identity</description>
                <run-as>
                    <description>Role to protect ALSB J2EE artifacts</description>
                    <role-name>ALSBSystem</role-name>
                </run-as>
           </security-identity>
      </message-driven>
  </enterprise-beans>
</ejb-jar>
